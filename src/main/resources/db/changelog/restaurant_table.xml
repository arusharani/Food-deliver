    <changeSet id="2" author="b">
    <createTable tableName="restaurants">

        <column name="restaurant_id" type="int" autoIncrement="true" startWith="501" incrementBy="1">
            <constraints primaryKey="true"/>
        </column>

        <column name="restaurant_name" type="varchar(50)">
            <constraints nullable="false"/>

        </column>

        <column name="contact_number" type="varchar(10)">
            <constraints nullable="false"/>
        </column>


        <column name="restaurant_address" type="varchar(70)">
            <constraints nullable="false"/>
        </column>

        <column name="created_at" type="datetime">
            <constraints nullable="false"/>
        </column>
        <column name="updated_at" type="datetime">
            <constraints nullable="false"/>
        </column>


    </createTable>

    </changeSet>

